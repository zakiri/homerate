{
  "name": "homerate-synthetic-exchange",
  "version": "1.0.0",
  "type": "module",
  "description": "Sentetik Emtia BorsasÄ± - OSMO Network Entegre",
  "main": "src/backend/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec babel-node src/backend/index.js",
    "dev:frontend": "next dev -p 3000",
    "build": "next build && npm run build:backend",
    "build:backend": "babel src/backend -d dist",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "node dist/index.js",
    "start:frontend": "next start -p 3000",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "jest",
    "migrate": "node src/backend/migrations/run.js",
    "seed": "node --experimental-modules src/backend/migrations/seedAssets.js"
  },
  "dependencies": {
    "@cosmjs/cosmwasm-stargate": "^0.31.0",
    "@cosmjs/proto-signing": "^0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.0",
    "framer-motion": "^10.16.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.6.3",
    "next": "^14.0.0",
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "tailwindcss": "^3.3.5",
    "uuid": "^9.0.0",
    "validator": "^13.11.0",
    "web3": "^4.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.29.0",
    "@babel/node": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.23.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
